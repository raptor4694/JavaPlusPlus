    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "JavaPlusPlus: debug module",
            "type": "python",
            "request": "launch",
            "args": ["-m", "unittest", "javapp"],
            "program": "",
            "console": "integratedTerminal"
        },
        {
            "name": "JavaPlusPlus: debug TestOptionalLiterals",
            "type": "python",
            "request": "launch",
            "args": ["javapp/test_files/TestOptionalLiterals/TestOptionalLiterals.javapp"],
            "program": "${workspaceFolder}/javapp.py",
            "console": "integratedTerminal"
        }
    ]
}